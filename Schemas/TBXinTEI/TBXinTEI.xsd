      <xs:sequence>
        <xs:group minOccurs="0" maxOccurs="unbounded" ref="ns2:model.entryPart"/>
        <xs:group minOccurs="0" maxOccurs="unbounded" ref="ns2:model.pLike"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ns2:nym"/>
      </xs:sequence>
      <xs:attributeGroup ref="ns2:att.global.attributes"/>
      <xs:attributeGroup ref="ns2:att.typed.attributes"/>
      <xs:attributeGroup ref="ns2:att.sortable.attributes"/>
      <xs:attribute name="parts">
        <xs:annotation>
          <xs:documentation>points to constituent nyms</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction>
            <xs:simpleType>
              <xs:list itemType="ns2:data.pointer"/>
